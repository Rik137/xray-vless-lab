{
  "log": {"loglevel":"warning"},
  "inbounds": [
    {
      "listen": "0.0.0.0",
      "port": 443,
      "protocol": "vless",
      "settings": {
        "clients": [
          {"id": "<UUId_here>"}
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "xhttp",
        "security": "reality",
        "realitySettings": {
          "show": false,
          "dest": "www.example.com:8443",
          "xver": 0,
          "serverNames": ["www.example.com"],
          "privateKey": "<your private key>",
          "shortIds": ["<xray id>"]
        },
        "xhttpSettings": {
          "host": "www.example.com",
          "path": "<random case>",
          "mode": "auto"
        }
      }
    }
  ],
  "outbounds": [
    {"protocol": "freedom","tag": "direct"}
  ]
}
